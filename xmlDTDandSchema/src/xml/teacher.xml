<?xml version="1.0" encoding="UTF-8"?>
<!-- 
xmlns:xsi:这里必须是这样的写法,也就是这个值已经是固定的
xmlns:这里是命名空间,也固定了,写的是schema里面的项目名称空间
xsi:schemalocation:有两段 前半段是名称空间(目标空间值),后面的是约束文档路径
 -->
<teachers
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.example.org/teacher"
	xsi:schemaLocation="http://www.example.org/teacher teacher.xsd">

	<teacher>
		<name>zhang</name>
		<age>13</age>
	</teacher>
	
	<teacher>
		<name>zhang</name>
		<age>13</age>
	</teacher>
	
	<teacher>
		<name>zhang</name>
		<age>13</age>
	</teacher>
</teachers>